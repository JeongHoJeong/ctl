{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "ES5",
    "baseUrl": "./",
    "outDir": "./bin"
  },
  "include": ["src/**/*"]
}
